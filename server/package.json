{
  "name": "flowkan-server",
  "version": "1.0.0",
  "main": "./index.js",
  "scripts": {
    "build": "tsc",
    "start": "concurrently  \"node dist/src/index.js\" \"node dist/src/worker.js\"",
    "dev:server": "tsx watch src/index.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:worker\"",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:init": "docker exec -it flowkan-server npx tsx src/init-db.ts",
    "db:migrate": "docker exec -it flowkan-server npx prisma migrate dev --name init",
    "db": "npm run db:migrate && npm run db:seed && npm run db:init",
    "db:reset": "docker exec -it flowkan-server npx prisma migrate reset && npx prisma generate && npm run db:init",
    "db:prod": "docker exec -it flowkan-server npx prisma migrate deploy",
    "db:seed": "docker exec -it flowkan-server npx tsx prisma/seed.ts",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "6.17.1",
    "@react-email/components": "1.0.1",
    "@react-email/render": "2.0.0",
    "@sentry/node": "10.30.0",
    "@sentry/profiling-node": "10.30.0",
    "amqplib": "0.10.9",
    "bcrypt": "6.0.0",
    "concurrently": "9.2.1",
    "cors": "2.8.5",
    "date-fns": "4.1.0",
    "dotenv": "17.2.3",
    "ejs": "3.1.10",
    "express": "5.2.1",
    "http-errors": "2.0.1",
    "jsonwebtoken": "9.0.3",
    "morgan": "1.10.1",
    "multer": "2.0.2",
    "passport": "0.7.0",
    "passport-github2": "0.1.12",
    "passport-google-oauth20": "2.0.0",
    "pg": "8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "resend": "6.6.0",
    "sharp": "0.34.5",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1",
    "xss": "1.0.15",
    "zod": "4.1.13"
  },
  "devDependencies": {
