{
  "name": "flowkan-server",
  "version": "1.0.0",
  "main": "./index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:init": "docker exec -it flowkan-server npx tsx src/init-db.ts",
    "db:migrate": "docker exec -it flowkan-server npx prisma migrate dev --name init",
    "db": "npm run db:migrate && npm run db:init",
    "db:reset": "docker exec -it flowkan-server npx prisma migrate reset && npx prisma generate && npm run db:init",
    "test": "echo \"Error: no test specified\" && exit 1"    
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "6.15.0",
    "bcrypt": "6.0.0",
    "cors": "2.8.5",
    "dotenv": "16.4.7",
    "express": "5.1.0",
    "http-errors": "2.0.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.1",
    "multer": "2.0.2",
    "pg": "8.16.3",
    "react": "19.1.1",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "9.31.0",
    "@types/bcrypt": "6.0.0",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/http-errors": "2.0.5",
    "@types/jsonwebtoken": "9.0.10",
    "@types/morgan": "1.9.10",
    "@types/multer": "2.0.0",
    "@types/node": "24.3.0",
    "@types/pg": "8.15.5",
    "@typescript-eslint/eslint-plugin": "8.41.0",
    "@typescript-eslint/parser": "8.41.0",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.5",
    "globals": "16.3.0",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "prisma": "6.15.0",
    "ts-node": "10.9.2",
    "tsx": "4.20.5",
    "typescript": "5.8.3",
    "typescript-eslint": "8.41.0"
  }
}
